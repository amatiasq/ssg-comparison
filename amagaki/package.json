{
  "name": "amagaki",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "browsersync:reload": "browser-sync reload",
    "browsersync:start": "browser-sync start --proxy localhost:8080 --files dist",
    "build": "npm run build:src; amagaki build",
    "build:prod": "npm run build:src; amagaki build -e prod",
    "build:src": "npm run gulp:build",
    "gulp:watch": "gulp watch",
    "gulp:build": "gulp build",
    "dev": "npm-run-all --parallel watch:pages gulp:watch browsersync:start start",
    "fix": "npm run fix:sass",
    "fix:sass": "sass-lint-auto-fix -c .sasslintrc.fix.yaml",
    "postinstall": "npm run build:src",
    "stage": "npm run build; npx @blinkk/fileset upload build",
    "start": "amagaki serve",
    "watch:src": "nodemon --watch src -e ts,js,sass --exec npm run browsersync:reload",
    "watch:pages": "nodemon --watch content --watch views -e njk,yaml --exec npm run browsersync:reload"
  },
  "dependencies": {
    "@amagaki/amagaki": "^1.12.1",
    "@amagaki/amagaki-engine-preact": "^1.1.1",
    "@amagaki/amagaki-plugin-page-builder": "^3.5.0",
    "@blinkk/degu": "^2.0.0",
    "@github/details-dialog-element": "^3.1.3",
    "browser-sync": "^2.27.5",
    "esbuild": "^0.13.15",
    "gts": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-dart-sass": "^1.0.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "preact": "^10.6.4",
    "preact-render-to-string": "^5.1.19",
    "quicklink": "^2.2.0",
    "sucrase": "^3.20.3",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "0.0.33",
    "@types/gulp-dart-sass": "^1.0.1",
    "glob": "^7.2.0"
  }
}